{
  "swagger": "2.0",
  "info": {
    "description": "APIs for Capacity Database",
    "version": "1.0.0",
    "title": "Capacity Database",
    "contact": {
      "email": "engineering@cargochief.com"
    }
  },
  "basePath": "/v1",
  "schemes": [
    "http"
  ],
  "tags": [
    {
      "name": "capacity",
      "description": "Truck Capacity based APIs"
    }
  ],
  "paths": {
    "/quote": {
      "post": {
        "tags": [
          "capacity"
        ],
        "summary": "Get the rate for a quote",
        "description": "",
        "operationId": "getQuote",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "lane",
            "description": "",
            "schema": {
              "$ref": "#/definitions/Lane"
            },
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "retrieved quote",
            "schema": {
              "$ref": "#/definitions/Quote"
            }
          }
        }
      }
    },
    "/searchCarrier": {
      "post": {
        "tags": [
          "capacity"
        ],
        "summary": "Get Carriers for a lane",
        "description": "",
        "operationId": "searchCarriers",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "lane",
            "description": "",
            "schema": {
              "$ref": "#/definitions/Lane"
            },
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "carrier list",
            "schema": {
              "$ref": "#/definitions/CarrierList"
            }
          }
        }
      }
    },
    "/searchTruck": {
      "post": {
        "tags": [
          "capacity"
        ],
        "summary": "Get trucks for a given lane",
        "description": "",
        "operationId": "searchTrucks",
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "lane",
            "description": "",
            "schema": {
              "$ref": "#/definitions/Lane"
            },
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "trucks list",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Truck"
              }
            }
          }
        }
      }
    },
    "/registerLoad": {
      "post": {
        "tags": [
          "capacity"
        ],
        "summary": "Register load to be maintained by Cargo Chief",
        "description": "",
        "operationId": "registerLoad",
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "load",
            "description": "",
            "schema": {
              "$ref": "#/definitions/Load"
            },
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "registered load successfully",
            "schema": {
              "$ref": "#/definitions/LoadReference"
            }
          }
        }
      }
    },
    "/getBids": {
      "post": {
        "tags": [
          "capacity"
        ],
        "summary": "Get bids for a registered load",
        "description": "",
        "operationId": "getBids",
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "loadReferenceNumber",
            "description": "",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Bid"
              }
            }
          }
        }
      }
    },
    "/trackLoad": {
      "post": {
        "tags": [
          "capacity"
        ],
        "summary": "Track a load through it's different states",
        "description": "",
        "operationId": "trackLoad",
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "loadReferenceNumber",
            "description": "",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "schema": {
              "$ref": "#/definitions/LoadTrackingInfo"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "LoadTrackingInfo": {
      "properties": {
        "loadReferenceNumber": {
          "type": "string",
          "example": "111LL"
        },
        "currentLocation": {
          "$ref": "#/definitions/Address"
        },
        "historicalLocations": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Address"
          }
        },
        "eta": {
          "type": "number",
          "format": "int64",
          "example": 1440
        },
        "currentStatus": {
          "type": "string",
          "enum": [
            "IN_TRANSIT"
          ]
        },
        "statusHistory": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/LoadStatus"
          }
        }
      }
    },
    "LoadStatus": {
      "properties": {
        "status": {
          "type": "string",
          "enum": [
            "CONFIRMED"
          ]
        },
        "time": {
          "type": "string",
          "format": "date-time",
          "example": "2017-02-26 T 14:45"
        }
      }
    },
    "Bid": {
      "properties": {
        "bidReferenceNumber": {
          "type": "string",
          "example": "123"
        },
        "estimatedDaysToDeliver": {
          "type": "number",
          "format": "int32",
          "example": 3
        },
        "cost": {
          "type": "number",
          "format": "double",
          "example": 59.90
        },
        "carrier": {
          "schema": {
            "$ref": "#/definitions/Carrier"
          }
        }
      }
    },
    "LoadReference": {
      "properties": {
        "referenceNumber": {
          "type": "string",
          "example": "1123LL"
        }
      }
    },
    "Customer": {
      "properties": {
        "businessName": {
          "type": "string",
          "example": "ABC Apples"
        },
        "address": {
          "schema": {
            "$ref": "#/definitions/Address"
          }
        },
        "contact": {
          "schema": {
            "$ref": "#/definitions/Contact"
          }
        }
      }
    },
    "Load": {
      "properties": {
        "lane": {
          "$ref": "#/definitions/Lane"
        },
        "loadStops": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/LoadStop"
          }
        },
        "carrier": {
          "$ref": "#/definitions/Carrier"
        },
        "customer": {
          "$ref": "#/definitions/Customer"
        },
        "lineItems": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/LineItem"
          }
        },
        "trackingNeeded": {
          "type": "boolean",
          "example": true
        },
        "bookingNeeded": {
          "type": "boolean",
          "example": true
        }
      }
    },
    "LineItem": {
      "properties": {
        "price": {
          "type": "number",
          "format": "double",
          "example": 50.75
        },
        "cost": {
          "type": "number",
          "format": "double",
          "example": 45.00
        },
        "chargeType": {
          "$ref": "#/definitions/ChargeType"
        }
      }
    },
    "Quote": {
      "properties": {
        "lane": {
          "$ref": "#/definitions/Lane"
        },
        "ratePerMile": {
          "type": "number",
          "format": "double",
          "example": 15.1
        },
        "totalPrice": {
          "type": "number",
          "format": "double",
          "example": 1270.0
        },
        "totalMiles": {
          "type": "number",
          "format": "double",
          "example": 100.0
        },
        "availableTrucks": {
          "type": "integer",
          "format": "int64",
          "example": 10
        },
        "priceGuaranteedTill": {
          "type": "string",
          "format": "date",
          "example": "2017-02-28"
        },
        "laneConfidence": {
          "type": "number",
          "format": "double",
          "example": 85.0
        }
      }
    },
    "Carrier": {
      "properties": {
        "businessName": {
          "type": "string",
          "example": "Cargo Chief Carriers"
        },
        "firmMc": {
          "type": "string",
          "example": "775132"
        },
        "firmDot": {
          "type": "string",
          "example": "193801"
        },
        "noOfTrucks": {
          "type": "integer",
          "format": "int32",
          "example": 10
        },
        "yearsOfService": {
          "type": "integer",
          "format": "int32",
          "example": 1
        },
        "loadsToDate": {
          "type": "integer",
          "format": "int32",
          "example": 50
        },
        "contact": {
          "schema": {
            "$ref": "#/definitions/Contact"
          }
        },
        "qualifiers": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Qualifier"
          }
        }
      }
    },
    "ChargeType": {
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "LINE_HAUL",
            "ACCESSORIALS",
            "ADDITIONAL_PALLETS",
            "AFTER_HOUR_DELIVERIES",
            "LAYOVERS",
            "DENTENTION_AT_LOADING"
          ]
        },
        "description": {
          "type": "string",
          "example": "Line Haul"
        }
      }
    },
    "Qualifier": {
      "properties": {
        "type": {
          "type": "string",
          "example": "BETTER_SERVICE"
        },
        "description": {
          "type": "string",
          "example": "Better Service"
        },
        "value": {
          "type": "number",
          "format": "double",
          "example": 4.5
        }
      }
    },
    "CarrierList": {
      "properties": {
        "lane": {
          "$ref": "#/definitions/Lane"
        },
        "ratePerMile": {
          "type": "number",
          "format": "double",
          "example": 15.1
        },
        "totalPrice": {
          "type": "number",
          "format": "double",
          "example": 1270.0
        },
        "totalMiles": {
          "type": "number",
          "format": "double",
          "example": 100.0
        },
        "availableTrucks": {
          "type": "integer",
          "format": "int64",
          "example": 10
        },
        "carriers": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Carrier"
          }
        }
      }
    },
    "Truck": {
      "properties": {
        "carrier": {
          "$ref": "#/definitions/Carrier"
        },
        "availableTrucks": {
          "type": "integer",
          "format": "int64",
          "example": 2
        },
        "availabilityConfidence": {
          "type": "number",
          "format": "double",
          "example": 85.0
        }
      }
    },
    "Lane": {
      "properties": {
        "origin": {
          "$ref": "#/definitions/Address"
        },
        "destination": {
          "$ref": "#/definitions/Address"
        },
        "pickupDate": {
          "type": "string",
          "format": "date",
          "example": "2017-02-27"
        },
        "deliveryDate": {
          "type": "string",
          "format": "date",
          "example": "2017-03-02"
        },
        "trailerType": {
          "type": "string",
          "enum": [
            "VAN",
            "REEFER",
            "FLATBED"
          ]
        },
        "product": {
          "type": "string",
          "example": "Apples"
        },
        "insurance": {
          "type": "number",
          "format": "double",
          "example": 1735.50
        },
        "specialInstructions": {
          "type": "string"
        },
        "carrierPreference": {
          "type": "string"
        }
      }
    },
    "LoadStop": {
      "properties": {
        "address": {
          "schema": {
            "$ref": "#/definitions/Address"
          }
        },
        "type": {
          "type": "string",
          "enum": [
            "PICKUP",
            "DELIVERY"
          ]
        },
        "referenceNumber": {
          "type": "string",
          "example": "123LH"
        },
        "scheduledTime": {
          "type": "string",
          "format": "date",
          "example": "2017-03-03"
        },
        "sequence": {
          "type": "number",
          "format": "int32",
          "example": "2"
        },
        "specialInstructions": {
          "type": "string",
          "example": "Pick this up too"
        }
      }
    },
    "Address": {
      "properties": {
        "address1": {
          "type": "string",
          "example": "1060 La Avenida Street"
        },
        "address2": {
          "type": "string"
        },
        "city": {
          "type": "string",
          "example": "Mountain View"
        },
        "state": {
          "type": "string",
          "example": "CA"
        },
        "zip": {
          "type": "string",
          "example": "94043"
        },
        "country": {
          "type": "string",
          "example": "US"
        },
        "latitude": {
          "type": "number",
          "format": "double",
          "example": 37.413691
        },
        "longitude": {
          "type": "number",
          "format": "double",
          "example": -122.071206
        }
      }
    },
    "Contact": {
      "properties": {
        "firstName": {
          "type": "string",
          "example": "John"
        },
        "lastName": {
          "type": "string",
          "example": "Smith"
        },
        "primaryPhone": {
          "type": "string",
          "example": "2342342342"
        },
        "primaryEmail": {
          "type": "string",
          "example": "john.smith@email.com"
        }
      }
    }
  }
}